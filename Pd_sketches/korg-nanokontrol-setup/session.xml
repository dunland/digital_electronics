<!DOCTYPE qjackctlSession>
<session name="korg-nanokontrol-setup">
 <client name="PulseAudio JACK Sink">
  <port name="front-left" type="out">
   <connect port="playback_1" client="system"/>
  </port>
  <port name="front-right" type="out">
   <connect port="playback_2" client="system"/>
  </port>
 </client>
 <client name="system">
  <port name="capture_1" type="out">
   <connect port="front-left" client="PulseAudio JACK Source"/>
   <connect port="input0" client="pure_data_0"/>
  </port>
  <port name="capture_2" type="out">
   <connect port="front-right" client="PulseAudio JACK Source"/>
   <connect port="input1" client="pure_data_0"/>
  </port>
  <port name="playback_1" type="in">
   <connect port="front-left" client="PulseAudio JACK Sink"/>
   <connect port="output0" client="pure_data_0"/>
  </port>
  <port name="monitor_1" type="out"/>
  <port name="playback_2" type="in">
   <connect port="front-right" client="PulseAudio JACK Sink"/>
   <connect port="output1" client="pure_data_0"/>
  </port>
  <port name="monitor_2" type="out"/>
 </client>
 <client name="pure_data_0">
  <port name="input0" type="in">
   <connect port="capture_1" client="system"/>
  </port>
  <port name="input1" type="in">
   <connect port="capture_2" client="system"/>
  </port>
  <port name="output0" type="out">
   <connect port="playback_1" client="system"/>
  </port>
  <port name="output1" type="out">
   <connect port="playback_2" client="system"/>
  </port>
 </client>
 <client name="PulseAudio JACK Source">
  <port name="front-left" type="in">
   <connect port="capture_1" client="system"/>
  </port>
  <port name="front-right" type="in">
   <connect port="capture_2" client="system"/>
  </port>
 </client>
</session>

